{
  "manifest_version": 3,
  "name": "Paris Olympics 2024 Stats",
  "version": "1.0.0",
  "description": "Paris summer olympics 2024 Leaderboard/Medal Tracker",
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
